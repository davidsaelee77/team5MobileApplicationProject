<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.HomeFragment">

    <ImageView
        android:id="@+id/placeholder_logo_imageview"
        android:layout_width="409dp"
        android:layout_height="98dp"
        android:layout_marginTop="88dp"
        android:layout_marginBottom="112dp"
        android:src="@drawable/ic_crop_16_9_black_24dp"
        app:layout_constraintBottom_toTopOf="@+id/placeholder_icon_imageview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/placeholder_icon_imageview"
        android:layout_width="409dp"
        android:layout_height="315dp"
        android:layout_marginTop="112dp"
        android:src="@drawable/temp_box_clip_art_black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/placeholder_logo_imageview" />
</androidx.constraintlayout.widget.ConstraintLayout>